    "watch": "run-p \"watch:**\""
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "browser-sync": "^3.0.4",
    "cross-env": "^10.1.0",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@fancyapps/ui": "^6.1.7",
    "aos": "^2.3.4",
    "echarts": "^6.0.0",
    "gsap": "^3.14.2"
  }
}
